{
  "name": "morganaw.com",
  "version": "2.0.0",
  "description": "Personal website for me (Morgan Aw)!",
  "main": "server.js",
  "scripts": {
    "server-dev": "NODE_ENV=development node ./build/server/server",
    "preserver-dev": "npm run clean-server && npm run build-server",
    "server-prod": "NODE_ENV=production node ./build/server/server",
    "preserver-prod": "npm run clean-server && npm run build-server",
    "client-dev": "NODE_ENV=development webpack -w --progress --color",
    "preclient-dev": "npm run clean-client",
    "client-prod": "NODE_ENV=production webpack -w --progress --color -p",
    "preclient-prod": "npm run clean-client",
    "clean-client": "rm -rf ./static/webpack",
    "clean-server": "rm -rf ./build",
    "build-server": "babel ./src -d ./build --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MorganCAw/morganaw.com.git"
  },
  "keywords": [
    "software",
    "developer",
    "javascript",
    "node",
    "react",
    "python",
    "django",
    "front-end",
    "back-end",
    "full-stack",
    "engineer"
  ],
  "author": "Morgan Aw",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MorganCAw/morganaw.com/issues"
  },
  "homepage": "https://github.com/MorganCAw/morganaw.com#readme",
  "dependencies": {
    "express": "^4.13.4",
    "pug": "^2.0.0-alpha6",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.6.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
